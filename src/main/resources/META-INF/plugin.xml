    <id>com.ly.mvp</id>
    <name>mvpTemplate</name>
    <description>使用该模板快捷创建Activity,layout,MVP等文件使用该模板快捷创建Activity,layout,MVP等文件</description>
    <vendor>liuyu</vendor>

    <!-- Product and plugin compatibility requirements -->
    <!-- https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html -->
    <depends>com.intellij.modules.platform</depends>
    <depends>org.jetbrains.android</depends>
    <depends>org.jetbrains.kotlin</depends>
    <depends>com.intellij.modules.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <applicationService serviceImplementation="com.allin.medrecorddossier.services.MyApplicationService"/>
        <projectService serviceImplementation="com.allin.medrecorddossier.services.MyProjectService"/>
    </extensions>
    <extensions defaultExtensionNs="com.android.tools.idea.wizard.template">
<!--        <wizardTemplateProvider implementation="other.allin.SamplePluginTemplateProviderImpl"/>-->
        <wizardTemplateProvider implementation="other.mvp.MVPTemplateProviderImpl"/>
    </extensions>

    <applicationListeners>
        <listener class="com.allin.medrecorddossier.listeners.MyProjectManagerListener"
                  topic="com.intellij.openapi.project.ProjectManagerListener"/>
    </applicationListeners>
</idea-plugin>
